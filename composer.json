{
  "name": "geeshoe/db-connector",
  "description": "PHP Database connection library.",
  "keywords": [
    "geeshoe",
    "dblib",
    "dbconnector",
    "mysql",
    "mariadb",
    "pdo",
    "php",
    "database"
  ],
  "homepage": "http://www.geeshoe.com",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "Jesse Rushlow",
      "email": "jr@geeshoe.com",
      "homepage": "http://geeshoe.com",
      "role": "Developer"
    }
  ],
  "require" : {
    "php" : "^7.2",
    "ext-pdo_mysql" : "*",
    "ext-pdo": "*"
  },
  "autoload": {
    "psr-4": {
      "Geeshoe\\DbConnector\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Geeshoe\\DbConnectorTest\\": "tests"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^8",
    "mikey179/vfsstream": "^1.6",
    "symfony/dotenv": "^4.2",
    "roave/security-advisories": "dev-master",
    "vimeo/psalm": "^3.11",
    "friendsofphp/php-cs-fixer": "^2.16"
  }
}
